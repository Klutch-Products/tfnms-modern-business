{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": ["resources/js/*"]
        },
        "module": "ESNext",
        "moduleResolution": "node",
        "allowJs": true
    },
    "exclude": ["node_modules", "public"],
    "include": [
        "resources/js/**/*.vue",
        "resources/js/**/*.js"
    ]
}
